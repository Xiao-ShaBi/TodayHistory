.containers {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  background-color: #f6f6f6;
  box-sizing: border-box;
}

.header {
  position: relative;
  display: flex;
  flex-direction: column;
  
  width: 100%;
  height: 140px;
  background-color: #f5f0e9;
  z-index: 99;
  -webkit-box-sizing: border-box;
  box-sizing: border-box;
  -webkit-box-shadow: 0px 2px 2px 0px rgba(0, 0, 0, 0.10);
  box-shadow: 0px 2px 2px 0px rgba(0, 0, 0, 0.10);
}

.picker {
  padding: 13px;
  background-color: #fff;
}

.header1 {
  position: relative;
  display: flex;
  flex-direction: row;
  width: 100%;
  height: 120px;
  background-color: #f5f0e9;
}

.notToday {
  padding-top: 10px;
}

.prev {
  position: absolute;
  left: 15px;
  top:0px;

background-position: center;
width:24px;
height:100%;
  background-image: url("data:image/png;base64,");
  background-repeat: no-repeat;
  background-size: 12px 21px;
}
.arrow{
  position: relative;
  width: 14px;
  height: 14px;
  left: 52px;
  top: 30px;
  background-image: url("data:image/png;base64,");
  background-repeat: no-repeat;
  background-size: 8px 8px;
}

.next {
  position: absolute;
  right: 15px;

top:0px;
background-position: center;
width:24px;
height:100%;

  background-image: url("data:image/png;base64,");
  background-repeat: no-repeat;
  background-size: 12px 21px;
}

.todayBtn {
  position: absolute;
  top: 50%;
  margin-top: -11px;
  right: 70px;
  background: #ff9447;
  width: 22px;
  height: 22px;
  border-radius: 22px;
  font-size: 12px;
  color: #fff;
  line-height: 22px;
  text-align: center;
}

.todayBtnHide {
  position: absolute;
  top: 50%;
  margin-top: -11px;
  right: 70px;
  background: #eb4646;
  width: 22px;
  height: 22px;
  border-radius: 22px;
  font-size: 12px;
  color: #fff;
  line-height: 22px;
  text-align: center;
  display: none;
}

.wrap1 {
  position: relative;
  padding: 5px;
  margin: 0 auto;
  background: rgba(255, 148, 71, 0.1);
  width: 90px;
  height: 90px;
  border-radius: 100%;
}

.wrap2 {
  padding: 10px;
  background: rgba(255, 148, 71, 0.2);
  width: 70px;
  height: 70px;
  border-radius: 100%;
}

.dateInfo {
  position: relative;
  background: #ff9447;
  box-shadow: 0px 0px 10px 0px rgba(255, 147, 71, 0.60);
  width: 70px;
  height: 70px;
  border-radius: 100%;
}

.year {
  position: absolute;
  width: 100%;
  left: 0;
  top: 5px;
  font-size: 13px;
  color: #fff;
  text-align: center;
}

.day {
  position: absolute;
  width: 100%;
  left: 0;
  top: 17px;
  font-size: 32px;
  color: #fff;
  line-height: 36px;
  text-align: center;
}

.month {
  position: absolute;
  width: 100%;
  left: 0;
  bottom: 6px;
  font-size: 13px;
  color: #fff;
  line-height: 13px;
  text-align: center;
}

.dayCount {
  position: relative;
  top: -10px;
  font-size: 13px;
  color: #777;
  line-height: 13px;
  text-align: center;
}

.historyrow {
 display: flex;
  align-items: center;
  text-align: left;
  width: 100%;

}

.block {
  display: flex;
  flex-direction: column;
  width: 100%;
  background-color: #f6f6f6;
}
.ll{
position: relative;
  min-width: 60px;
  text-align: center;
}

.historytime {

color: #777777;
font-size: 13px;
  
}
.historytime1 {

color: #333333;
font-size: 13px;
  
