}.textparent{
  position: relative;
  margin-left: 15px;
  margin-right: 15px;
  overflow: hidden;
  box-shadow: 0 2px 0 0 rgba(0, 0, 0, 0.04);
  border-radius: 100px;
  line-height: 1.27;
  background: #fff;
}
.text {
position: relative; 
margin-top: 10px;
margin-bottom: 10px;
margin-left: 15px;
margin-right: 15px;
  overflow: hidden;
  line-height: 1.27;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 3;
  -webkit-box-orient: vertical;
  text-align: left;
  color: #333333;

}
.text1 {
  position: relative; 

margin-top: 10px;
